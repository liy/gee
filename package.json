{
  "name": "gee",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "concurrently \"npm run start-electron\" \"npm run watch-web\"",
    "start-electron": "node esbuild-src.js && electron .",
    "watch-web": "node esbuild-web.js",
    "typecheck": "tsc -p ./tsconfig.json",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "rebuild-native": "electron-rebuild -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liy/gee.git"
  },
  "author": {
    "name": "liy",
    "email": "liy8272@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liy/gee/issues"
  },
  "homepage": "https://github.com/liy/gee#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_template"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/minimist": "^1.2.1",
    "@types/nodegit": "^0.26.12",
    "@types/stats.js": "^0.17.0",
    "@types/xterm": "^3.0.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-rebuild": "^2.3.5",
    "electron-reload": "^1.5.0",
    "esbuild": "^0.8.57",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.20.0",
    "prettier": "^2.2.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@pixi/unsafe-eval": "^6.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "module-alias": "^2.2.2",
    "node-pty": "^0.10.0",
    "nodegit": "^0.27.0",
    "os": "^0.1.1",
    "pixi.js": "^6.0.0",
    "stats.js": "^0.17.0",
    "xterm": "^4.11.0"
  }
}
